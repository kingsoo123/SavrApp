{
  "expo": {
    "name": "Savr",
    "slug": "Savr",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "packagerOpts":{
      "nonPersistent": true
    },
    "ios": {
      "supportsTablet": true
    },
    "description": "This Mobile App let's users save",
    "githubUrl": "https://github.com/kingsleyOnyelo/Savr"
  }
}